	<queryString language="SQL">
		<![CDATA[select  NOM_COMPLETO,    CASE
        WHEN TIMESTAMPDIFF(YEAR,  DAT_NASCIMENTO,CURDATE())  BETWEEN 12 and 13 THEN 'Polem'
        WHEN TIMESTAMPDIFF(YEAR,  DAT_NASCIMENTO,CURDATE()) BETWEEN 13 and 14 THEN 'Sementes'
        WHEN TIMESTAMPDIFF(YEAR,  DAT_NASCIMENTO,CURDATE()) BETWEEN 15 and 16 THEN 'Flores'
        WHEN TIMESTAMPDIFF(YEAR,  DAT_NASCIMENTO,CURDATE()) BETWEEN 17 and 18 THEN 'Colheita'
		WHEN TIMESTAMPDIFF(YEAR,  DAT_NASCIMENTO,CURDATE()) BETWEEN 18 and 26 THEN 'Tarefeiros do Bem'
    END as GRUPO  from  comeerj
	where DSC_TIPO_PARTICIPACAO = 'Confraternista'
	order by GRUPO, NOM_COMPLETO;]]>
	</queryString>
